<?xml version="1.0"?>
<ruleset name="SQWP">
    <description>Custom WordPress Ruleset</description>

    <rule ref="WordPress-Core">
        <properties>
            <property name="strict_class_file_names" value="false" />
        </properties>
    </rule>

    <rule ref="Generic.Arrays.DisallowShortArraySyntax.Found">
        <type>warning</type>
    </rule>

</ruleset>
